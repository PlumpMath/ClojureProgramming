(ns protocols.core)

(defn foo
  "I don't do a whole lot."
  [x]
  (println x "Hello, World!"))

(defprotocol IHello
  (hello1 [this a b])
  (hello [this a b & more]))

(deftype Hello []
  IHello
  (hello1 [this a b]
    (println a b))
  (hello [this a b & more]
    (println more)))

(def a (Hello.))
(hello1 a 1 2)
(hello a 1 2 3)


